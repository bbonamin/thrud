<h1>Listing exercise_logs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Sets</th>
    <th>Repetitions</th>
    <th>Weight</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exercise_logs.each do |exercise_log| %>
  <tr>
    <td><%= exercise_log.name %></td>
    <td><%= exercise_log.sets %></td>
    <td><%= exercise_log.repetitions %></td>
    <td><%= exercise_log.weight %></td>
    <td><%= link_to 'Show', exercise_log %></td>
    <td><%= link_to 'Edit', edit_exercise_log_path(exercise_log) %></td>
    <td><%= link_to 'Destroy', exercise_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Exercise log', new_exercise_log_path %>
